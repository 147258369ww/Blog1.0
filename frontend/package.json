{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:https": "vite --mode production",
    "build": "vue-tsc -b && vite build --mode production",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "preview": "vite preview",
    "preview:https": "vite preview --mode production --port 5173 --host",
    "start": "vite preview --port 5173 --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "verify": "node scripts/verify-integration.js",
    "test:integration": "vite build && node scripts/verify-integration.js"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "gsap": "^3.13.0",
    "lenis": "^1.3.15",
    "marked": "^16.4.1",
    "ogl": "^1.0.11",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^3.1.0"
  }
}
